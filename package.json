{
  "name": "expense-splitter-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prodtest": "ng serve --prod --aot",
    "clean": "rimraf ./dist",
    "build:prod": "ng build --prod",
    "build:prodaot": "ng build --prod --aot",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:nowatch": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "s3:delete": "aws s3 rm s3://angular-expenses-splitter --recursive",
    "s3:upload": "aws s3 cp ./dist/expense-splitter-frontend s3://angular-expenses-splitter --recursive --include \"*\"",
    "s3": "npm run s3:delete && npm run s3:upload",
    "firebase": "firebase deploy",
    "deploy": "npm run build:prodaot && npm run firebase",
    "build:ci": "npm run clean && npm run test && npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.12",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~9.1.12",
    "@angular/compiler": "~9.1.12",
    "@angular/core": "~9.1.12",
    "@angular/fire": "^5.2.3",
    "@angular/forms": "~9.1.12",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~9.1.12",
    "@angular/platform-browser-dynamic": "~9.1.12",
    "@angular/router": "~9.1.12",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "firebase": "^7.5.1",
    "jquery": "^3.5.0",
    "popper.js": "^1.15.0",
    "rxjs": "~6.6.3",
    "tesseract.js": "^2.0.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.12",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "~9.1.12",
    "@angular/language-service": "~9.1.12",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "aws-sdk": "^2.463.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~5.4.0",
    "puppeteer": "^2.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
