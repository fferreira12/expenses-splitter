{
  "name": "expense-splitter-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:prodtest": "ng serve --prod --aot",
    "clean": "rimraf ./dist",
    "build:prod": "ng build --prod",
    "build:prodaot": "ng build --prod --aot",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test-local": "ng test",
    "test:nowatch": "ng test --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "s3:delete": "aws s3 rm s3://angular-expenses-splitter --recursive",
    "s3:upload": "aws s3 cp ./dist/expense-splitter-frontend s3://angular-expenses-splitter --recursive --include \"*\"",
    "s3": "npm run s3:delete && npm run s3:upload",
    "firebase": "firebase deploy",
    "deploy": "npm run build:prodaot && npm run firebase",
    "build:ci": "npm run clean && npm run test && npm run build:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/cdk": "~10.0.0",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/fire": "^5.2.3",
    "@angular/forms": "~10.1.3",
    "@angular/material": "^10.0.0",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@angular/service-worker": "~10.1.3",
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@ngrx/effects": "^10.0.0",
    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.1",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "fast-copy": "^2.1.0",
    "firebase": "^7.5.1",
    "jquery": "~3.4.1",
    "jspdf": "^2.1.1",
    "jspdf-autotable": "^3.5.13",
    "popper.js": "^1.15.0",
    "rxjs": "~6.6.3",
    "tesseract.js": "^2.0.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "^10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@angular/language-service": "~10.1.3",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "aws-sdk": "^2.463.0",
    "codelyzer": "^5.1.2",
    "firebase-admin": "^9.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-spec-reporter": "0.0.32",
    "protractor": "~7.0.0",
    "puppeteer": "^2.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
